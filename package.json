{
  "name": "veeam-mtr",
  "version": "1.0.0",
  "description": "API de Monitoramento do Veeam Backup.",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "build:v2": "tsup",
    "start:prod": "node build/server.js",
    "auth": "tsx src/veeamAuth.ts",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "stop:pm2": "pm2 stop ecosystem.config.cjs",
    "restart:pm2": "pm2 restart ecosystem.config.cjs",
    "delete:pm2": "pm2 delete ecosystem.config.cjs",
    "logs:pm2": "pm2 logs ecosystem.config.cjs"
  },
  "keywords": [
    "veeam",
    "backup",
    "monitoramento",
    "api",
    "fastify",
    "nodejs",
    "typescript"
  ],
  "author": "2Â° Ten Guedes",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "fastify": "^5.4.0",
    "fastify-metrics": "^12.1.0",
    "node-fetch": "^2.7.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/node-fetch": "^2.6.12",
    "dotenv": "^17.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}